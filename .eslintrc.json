{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "import/extensions":"off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"]
    }
}
